{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "no-console": "off",
    "no-prototype-builtins": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "import/newline-after-import": "off",
    "import/extensions": ["error", "ignorePackages", {
      "js": "never",
      "jsx": "never"
    }]
  }
}
